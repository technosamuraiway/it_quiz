{
  "name": "it_quiz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build && pnpm run build-server",
    "dev": "vite",
    "server:dev": "ts-node-esm server/api.ts",
    "build-server": "tsc -p tsconfig.node.json",
    "start": "node -r dotenv/config dist/server/api.js",
    "format": "prettier --write src",
    "lint": "eslint src --no-error-on-unmatched-pattern --fix && stylelint --fix src/{,*/}*.{scss,css} --allow-empty-input",
    "build-deploy-date": "node scripts/build-deploy-date.js",
    "build-with-date": "npm run build-deploy-date && npm run build"
  },
  "dependencies": {
    "@it-incubator/prettier-config": "0.1.2",
    "@it-incubator/stylelint-config": "2.0.0",
    "@radix-ui/react-scroll-area": "1.2.3",
    "@radix-ui/react-select": "2.1.6",
    "@supabase/supabase-js": "^2.49.1",
    "bcryptjs": "^3.0.2",
    "classnames": "2.5.1",
    "clsx": "2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "7.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "serverless-http": "^3.2.0",
    "vite-plugin-pwa": "0.21.1",
    "workbox-window": "7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@types/pg": "^8.11.11",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react-swc": "3.8.0",
    "eslint": "8.57.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "prettier": "^3.5.3",
    "sass": "1.85.1",
    "stylelint": "16.14.1",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.24.1",
    "vite": "6.2.0"
  }
}
